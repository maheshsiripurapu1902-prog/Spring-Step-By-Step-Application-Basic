<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
        
        
        
        
        
        <bean class="com.java.setter.getters.application1.Student" id="id1">
        
        <property name="id">
        <value>1</value>
        </property>
        
        
        <property name="name" value="mahesh"/>
        <property name="address" value="vizag"/>
        <property name="email" value="mahesh@gmail.com"/>
        <property name="password">
         <value>mahesh@123</value>
        </property>
        </bean>
        
            
        <bean class="com.java.application2.Emp" id="id2">
    
    <constructor-arg name="id" value="2"/>
    <constructor-arg name="name" value="madhu"/>
    <constructor-arg name="email" value="madhu@gmail.com"/>
    <constructor-arg name="address" value="vzm"/>
    <constructor-arg name="password" value="madhu@1234"/>
    </bean>
        
        
        <bean class="com.java.collections.application3.Product" id="id3">
        
        <property name="pid" value="33"/>
        <property name="pname" value="laptop"/>
        
        <property name="price" value="80000"/>
        
       <property name="qty">
       
       <list>
       <value>1</value>
       <value>3</value>
       <value>4</value>
       <value>55</value>
       
       </list>
       
       </property>
       
       
       <property name="Email">
       
       <set>
       <value>Mahesh@gmail.com</value>
       <value>Madhu@gmail.com</value>
       <value>venky@gmail.com</value>
       <value>hariesh@gmail.com</value>
       
       
       </set>
       
       
       </property>
       
       <property name="items">
       
       <map>
       
       <entry key="1" value="mobile"/>
       <entry key="2" value="laptop"/>
       <entry key="3" value="tvs"/>
       <entry key="4" value="eletronics"/>
       </map>
       
       
       </property>
        
        </bean>
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        </beans>
        
        
        